{
  "name": "solaris",
  "version": "1.0.0",
  "description": "",
  "main": "build/js/solaris-bundle.js",
  "dependencies": {
    "babel": "^6.5.2",
    "babel-preset-env": "^1.3.3",
    "babel-preset-react": "^6.23.0",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "chance": "^1.0.6",
    "concurrently": "^3.4.0",
    "shx": "^0.2.2"
  },
  "devDependencies": {},
  "scripts": {
    "start": "http-server ./public",
    "start-n-watch": "concurrently --prefix \"[{name}]\" --names \"watch,server\" \"npm run watch\" \"npm start\"",
    "prewatch": "npm run build",
    "watch": "watchify ./src/solaris.js --verbose -t [ babelify --presets [ env ] ] --debug -o ./public/js/build/solaris-bundle.js",
    "prebuild": "shx mkdir -p ./public/js/build",
    "build": "browserify ./src/solaris.js -t [ babelify --presets [ env ] ] -o ./public/js/build/solaris-bundle.js"
  },
